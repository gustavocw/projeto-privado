{
  "name": "alkord_react",
  "version": "0.0.1",
  "private": true,
  "workspaces": {
    "packages": [
      "libs/*",
      "apps/*"
    ],
    "nohoist": [
      "**/react",
      "**/react/**",
      "**/react-dom",
      "**/react-dom/**",
      "typedjson"
    ]
  },
  "installConfig": {
    "hoistingLimits": "dependencies"
  },
  "scripts": {
    "tasks": "ts-node --project ./tsconfig.tsnode.json tasks.ts",
    "generate": "npm run tasks generate",
    "clean": "del-cli -f \"./**/tsconfig.generated.json\" && del-cli -f \"./apps/**/*.tsbuildinfo\" && npm run generate",
    "fullclean": "npm run clean && del-cli -f \"./**/node_modules\"",
    "postinstall": "npm run clean",
    "core:start": "vite serve apps/core --config ./vite.config.ts -m core.dev --force",
    "core:build": "vite build apps/core --config ./vite.config.ts -m core",
    "terceirizacao:start": "vite serve apps/terceirizacao --config ./vite.config.ts -m terceirizacao.dev --force",
    "terceirizacao:build": "vite build apps/terceirizacao --config ./vite.config.ts -m terceirizacao"
  },
  "dependencies": {
    "@date-io/moment": "1.x",
    "@material-ui/core": "4.11.2",
    "@material-ui/data-grid": "~4.0.0-alpha.33",
    "@material-ui/icons": "4.11.3",
    "@material-ui/lab": "4.0.0-alpha.57",
    "@material-ui/pickers": "3.2.10",
    "@material-ui/x-grid": "4.0.0-alpha.37",
    "@swc/helpers": "^0.4.14",
    "axios": "~0.21.0",
    "axios-auth-refresh": "~3.0.0",
    "classnames": "~2.2.6",
    "deep-equal": "~2.0.5",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "mobx": "~5.15.7",
    "mobx-react": "~6.3.1",
    "moment": "^2.24.0",
    "moment-mini": "^2.29.4",
    "notistack": "~1.0.5",
    "react": "^16.14.0",
    "react-data-grid": "~6.0.0",
    "react-dom": "^16.14.0",
    "react-number-format": "4.4.4",
    "react-refresh": "^0.14.0",
    "react-router-cache-route": "~1.10.1",
    "react-router-dom": "5.2.1",
    "react-text-mask": "~5.4.3",
    "typedjson": "^1.8.0",
    "uuid": "~9.0.0",
    "yup": "~0.32.8"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.20.2",
    "@swc/core": "^1.3.19",
    "@types/node": "^18.11.9",
    "@types/react": "16.14.26",
    "@types/react-dom": "^16.9.17",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "@vitejs/plugin-react": "^2.2.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "del-cli": "^5.0.0",
    "eslint": "~7.32.0",
    "eslint-config-google": "~0.14.0",
    "eslint-config-prettier": "~7.2.0",
    "eslint-plugin-prettier": "~3.4.1",
    "eslint-plugin-react": "~7.29.4",
    "eslint-plugin-react-hooks": "~4.5.0",
    "optionator": "^0.9.1",
    "rollup-plugin-visualizer": "^5.8.3",
    "sass": "^1.56.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "~4.7.3",
    "unplugin-swc": "^1.3.2",
    "vite": "^3.2.3",
    "vite-plugin-checker": "^0.5.1",
    "vite-plugin-swc-react-refresh": "^2.2.1"
  },
  "resolutions": {
    "@types/react": "16.14.26",
    "@types/lodash": "~4.14.165",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-refresh": "^0.14.0"
  },
  "packageManager": "yarn@3.2.4"
}
